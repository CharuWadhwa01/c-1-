#include<iostream>
using namespace std;
void  reverse(int arr[],int len){
     for (int i = 0; i < len/2; i++)
    {
        swap(arr[i],arr[len - 1 - i]);
    }
   
    
}
int main(){
   int arr[5],len=5;
   for ( int i = 0; i < len; i++)
   {
    cin>>arr[i];
   }
   for (int i = 0; i <len; i++)
   {
    cout<<arr[i]<<" ";
   }
   cout<<endl;
   reverse(arr,len);
   for (int i = 0; i <len; i++)
   {
      cout<<arr[i]<<" ";
   }
   cout<<endl;
   return 0;
}
