#include<stdio.h>
int main()
{
    int n,rema,sum=0,num=n;
    printf("enter a number n\n");
    scanf("%d",&n);

    while (num!=0)
    {
      rema=num%10;  
      sum+=rema*rema*rema;
      num=num/10;
    }
    
    
    if (sum==n)
    {
    printf("armstrong number\n");
    }
    else
    printf("not a armstrong number\n");
    return 0;
}

